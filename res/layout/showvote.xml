<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android" 
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Launch">


    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/topic"
    />

     <LinearLayout
        android:id="@+id/newvotes_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/description"
        android:orientation="vertical"
        >
             
    </LinearLayout>

    <TextView
        android:id="@+id/describe"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_x="-1dp"
        android:layout_y="47dp"
        android:text="@string/description" />

    <TextView
        android:id="@+id/right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="22dp"
        android:layout_y="211dp"
        android:text="@string/encrypted" />

    <TextView
        android:id="@+id/launcher"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="28dp"
        android:layout_y="312dp"
        android:text="@string/launcher" />

    <TextView
        android:id="@+id/timerequest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="28dp"
        android:layout_y="274dp"
        android:text="@string/timelimit" />

    <EditText
        android:id="@+id/de_vote_option_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="7dp"
        android:layout_y="108dp"
        android:ems="10"
        android:hint="@string/hint_option1" />

    <TextView
        android:id="@+id/choice"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_x="24dp"
        android:layout_y="173dp"
        android:text="@string/option" />

 </AbsoluteLayout>
