<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/activity_vertical_margin"
    tools:context=".Vote" >

	<TableLayout
	    android:id="@+id/whole"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content" >
	    
	    <TableRow>
			<TextView
			    android:id="@+id/text_voteid"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_marginRight="@dimen/activity_vertical_margin"
			    android:text="@string/voteid"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
			
			<TextView
			    android:id="@+id/voteid"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="@string/fill_white_space"
			    android:textAppearance="?android:attr/textAppearanceMedium" />

		</TableRow> 
		
	    <TableRow> 
			<TextView
			    android:id="@+id/text_launcher"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_marginRight="@dimen/activity_vertical_margin"
			    android:text="@string/launcher"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
			
			<TextView
			    android:id="@+id/launcher"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:text="@string/fill_white_space"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
		</TableRow> 
		
	    <TableRow> 
			<TextView
			    android:id="@+id/text_topic"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_marginRight="@dimen/activity_vertical_margin"
			    android:text="@string/topic"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
			
			<TextView
			    android:id="@+id/topic"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:text="@string/fill_white_space"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
		</TableRow>
		
	    <TableRow> 
			<TextView
			    android:id="@+id/text_vote_type"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_marginRight="@dimen/activity_vertical_margin"
			    android:text="@string/vote_type"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
			
			<TextView
			    android:id="@+id/vote_type"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:text="@string/fill_white_space"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
		</TableRow> 
		
	    <TableRow> 
			<TextView
			    android:id="@+id/text_deadline"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_marginRight="@dimen/activity_vertical_margin"
			    android:text="@string/deadline"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
			
			<TextView
			    android:id="@+id/deadline"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:text="@string/fill_white_space"
			    android:textAppearance="?android:attr/textAppearanceMedium" />
		</TableRow> 
	</TableLayout>
 </ScrollView>
