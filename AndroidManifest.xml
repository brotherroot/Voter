<!-- <?xml version="1.0" encoding="utf-8"?> -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"

        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.voter.MainCode"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.voter.Launch">
        </activity>
        <activity
            android:name="com.example.voter.Show">
        </activity>
        <activity
            android:name="com.example.voter.Password"
            android:label="@string/get_password" 
            android:theme="@android:style/Theme.Dialog" > 
        </activity>
        <activity
            android:name="com.example.voter.Join">
        </activity>
        
        <activity
            android:name="com.example.voter.Vote">
        </activity>
    </application>

</manifest>